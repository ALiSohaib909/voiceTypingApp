<?xml version="1.0" encoding="utf-8"?>
<Keyboard
    xmlns:android="http://schemas.android.com/apk/res/android"
          android:horizontalGap="1%p"
          android:keyHeight="@dimen/key_height"
          android:keyWidth="9.1%p"
          android:verticalGap="1%p">
    <Row
        android:rowEdgeFlags="top"
        android:horizontalGap="1%p"
        android:keyHeight="@dimen/key_for_digit"
        android:keyWidth="18.8%p">
        <Key android:codes="-3"
            android:keyIcon="@drawable/sym_keyboard_done"
            android:keyEdgeFlags="left"/>
        <Key
            android:codes="-999"
            android:keyIcon="@drawable/e056"/>
        <Key
            android:codes="-301"
            android:isModifier="true"
            android:isSticky="true"
            android:keyLabel="@string/hindi"/>
        <Key
            android:codes="-17"
            android:keyIcon="@drawable/themes_png"/>
        <Key
            android:codes="-16"
            android:isModifier="true"
            android:isSticky="true"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/settings_png"/>
    </Row>
    <Row android:keyHeight="@dimen/key_for_digit">
        <Key android:codes="64" android:keyLabel="\@"    android:keyWidth="9%p" android:keyEdgeFlags="left"/>
        <Key android:codes="35" android:keyLabel="\#"    android:keyWidth="9%p"  />
        <Key android:codes="36" android:keyLabel="$"     android:keyWidth="9%p"  />
        <Key android:codes="37" android:keyLabel="%"     android:keyWidth="9%p"  />
        <Key android:codes="38" android:keyLabel="&amp;" android:keyWidth="9%p"  />
        <Key android:codes="42" android:keyLabel="*"     android:keyWidth="9%p"  />
        <Key android:codes="45" android:keyLabel="-"     android:keyWidth="9%p"  />
        <Key android:codes="61" android:keyLabel="="     android:keyWidth="9%p"  />
        <Key android:codes="40" android:keyLabel="("     android:keyWidth="9%p"  />
        <Key android:codes="41" android:keyLabel=")"     android:keyWidth="9%p"  android:keyEdgeFlags="right"/>
    </Row>
    <Row>
        <Key android:keyWidth="9%p" android:codes="126"  android:keyLabel="~" android:horizontalGap="5%p"  android:keyEdgeFlags="left"  />
        <Key android:keyWidth="9%p" android:codes="177"  android:keyLabel="±" />
        <Key android:keyWidth="9%p" android:codes="215"  android:keyLabel="×" />
        <Key android:keyWidth="9%p" android:codes="247"  android:keyLabel="÷" />
        <Key android:keyWidth="9%p" android:codes="95" android:keyLabel="_" />
        <Key android:keyWidth="9%p" android:codes="96"   android:keyLabel="`" />
        <Key android:keyWidth="9%p" android:codes="180"  android:keyLabel="´" />
        <Key android:keyWidth="9%p" android:codes="123"  android:keyLabel="{" />
        <Key android:keyWidth="9%p" android:codes="125"  android:keyEdgeFlags="right" android:keyLabel="}" />
    </Row>
    <Row>
        <Key
            android:codes="-14"
            android:isModifier="true"
            android:isSticky="true"
            android:keyEdgeFlags="left"
            android:keyIcon="@drawable/shift"
            android:keyWidth="13.5%p" />
        <Key android:codes="47"  android:keyWidth="9%p" android:keyLabel="/" />
        <Key android:codes="191" android:keyWidth="9%p" android:keyLabel="¿" />
        <Key android:codes="58"  android:keyWidth="9%p" android:keyLabel=":" />
        <Key android:codes="59"  android:keyWidth="9%p" android:keyLabel=";" />
        <Key android:codes="44"  android:keyWidth="9%p" android:keyLabel="," />
        <Key android:codes="33"  android:keyWidth="9%p" android:keyLabel="!" />
        <Key android:codes="63"  android:keyWidth="9%p" android:keyLabel="\?" />
        <Key
            android:codes="-5"
            android:isRepeatable="true"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/delete"
            android:keyWidth="13.5%p" />
    </Row>
    <Row android:rowEdgeFlags="bottom" >
        <Key
            android:codes="-200"
            android:horizontalGap="1%p"
            android:keyEdgeFlags="left"
            android:keyWidth="14%p"
            android:keyIcon="@drawable/sys_button" />
        <Key
            android:codes="-14"
            android:keyWidth="12%p"
            android:keyLabel="@string/eng_ime" />
        <Key
            android:codes="32"
            android:keyIcon="@drawable/space"
            android:keyWidth="42%p" />
        <Key
            android:codes="46"
            android:keyWidth="12%p"
            android:keyLabel="."
            android:popupKeyboard="@xml/popup_punctuation" />
        <Key
            android:codes="10"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/next"
            android:keyWidth="14%p" />
    </Row>
</Keyboard>
